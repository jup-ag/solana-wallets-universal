{
  "name": "@solana-wallets/unified",
  "version": "0.3.4",
  "description": "Example hello world package in the solana-wallets repository.",
  "license": "MIT",
  "author": "aidanaden",
  "contributors": [],
  "homepage": "https://github.com/aidanaden/solana-wallets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aidanaden/solana-wallets.git"
  },
  "bugs": {
    "url": "https://github.com/aidanaden/solana-wallets/issues"
  },
  "keywords": [
    "solid"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./src/index.tsx",
  "exports": {
    ".": {
      "solid": {
        "development": "./dist/index.jsx",
        "import": "./dist/dev.jsx"
      },
      "development": {
        "import": {
          "types": "./src/index.tsx",
          "default": "./dist/dev.js"
        }
      },
      "import": {
        "types": "./src/index.tsx",
        "default": "./dist/index.js"
      }
    },
    "./index.css": {
      "development": {
        "import": "./dist/dev.css"
      },
      "import": "./dist/index.css"
    }
  },
  "browser": {},
  "typesVersions": {},
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test:client --mode ssr",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@corvu/dialog": "^0.2.3",
    "@nanostores/solid": "^0.4.2",
    "@solana-wallets/core-2.0": "workspace:*",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-standard-core": "1.1.1",
    "@solana/wallet-standard-wallet-adapter-base": "^1.1.2",
    "@solid-primitives/context": "^0.2.3",
    "autoprefixer": "^10.4.19",
    "nanostores": "^0.11.3",
    "postcss": "^8.4.38",
    "solid-element": "^1.8.1",
    "solid-js": "^1.8.18",
    "tailwindcss": "^3.4.3"
  },
  "peerDependencies": {
    "solid-js": "^1.8.18"
  }
}
